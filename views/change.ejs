    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <style>
        body{
            background-image: url("https://k39.kn3.net/taringa/3/3/8/1/3/7//aadamm/F6F.jpg?5702");
            background-size:cover;
        }
        .col-center-block {
            font-size: larger;
            float: none;
            display: block;
            margin-left: auto;
            margin-right: auto;
            margin-top: 300px;
        }

    </style>
</head>

<body>
<div class="container">
    <div class="row myCenter">
        <div class="col-xs-6 col-md-4 col-center-block">
            <form id="change" name="change" method="post" action="/change">
                <input name="uid" type="hidden" value="<%=uid%>">
                <label for="newname" class="sr-only"> New Fullname</label>
                <input name="newname" type="text" class="form-control" placeholder="newname" required autofocus/>
                <label for="newpassword" class="sr-only">New Password</label>
                <input name="newpassword" type="password" class="form-control" placeholder="newpassword" required autofocus/>
                <label for="newemail" class="sr-only">New Email</label>
                <input class="form-control" placeholder="email"  name="newemail" type="email" />
                <label for="newcity" class="sr-only">New City</label>
                <input class="form-control" name="newcity" type="city" onkeyup="this.value=this.value.replace(/[^a-zA-Z]/g,'')" placeholder="city" />
                <input class="btn btn-lg btn-primary btn-block" type="submit" value="Confirm" />
            </form>
        </div>
    </div>
</div>
</body>

</html>
